     if (errorsFromSocket > ERRORS_FROM_SOCKET_THRESHOLD) {\n            Debug.log('error out', event.data.message, event.data.details);\n        }\n        return;\n    }\n    window.parent.postMessage(event, '*');\n};\nconst initDirect = () => {\n    connect(self.location.host, processSocketEvent, { connectionMode: 'direct', appVersion: appConfig.appVersion });\n    /**\n     *       -\n     *     \n     */\n    if (isServiceWorkersAvailable()) {\n        setTimeout(() => {\n            if (isServiceWorkersAvailable()) {\n                swRegistrationAttempts = 0;\n                registerWorker();\n            }\n        }, WAIT_TILL_RETURN_TO_WORKER_REGISTRATION);\n    }\n};\n/**\n *       fallback-\n */\nconst workerRegistrationErrors = [];\nconst logWorkerRegistrationErrors = async () => {\n    const additionalData = {};\n    try {\n        const response = await fetch(SERVICE_WORKER_URL);\n        additionalData.workerContentType = response.headers.get('content-type');\n    }\n    catch (e) {\n        additionalData.fetchError = getErrorAdditionalData(e);\n    }\n    Debug.log('error out', 'Max service worker registration attempts', {\n        workerRegistrationErrors,\n        ...additionalData,\n    });\n    workerRegistrationErrors.length = 0;\n};\n/**\n *    /   \n *       \n *  ,    \n */\nconst processServiceWorkerFailure = () => {\n    if (swRegistrationAttempts < MAX_WORKER_REGISTRATION_ATTEMPTS && isServiceWorkersAvailable()) {\n        setTimeout(() => {\n            registerWorker();\n        }, WAIT_TILL_RESTART_WORKER);\n    }\n    else {\n        logWorkerRegistrationErrors();\n        initDirect();\n    }\n};\nconst registerWorker = async () => {\n    swRegistrationAttempts++;\n    navigator.serviceWorker.onmessage = (event) => {\n        /**\n         *      -      \n         */\n        swRegistrationAttempts = 0;\n        /**\n         *      ws-\n         *      \n         *    'ready'   appConfig\n         */\n        if (event.data === 'ready') {\n            navigator.serviceWorker.controller?.postMessage({ name: 'appConfig', data: appConfig });\n        }\n        processSocketEvent(event.data);\n    };\n    let worker;\n    try {\n        worker = await navigator.serviceWorker.register(SERVICE_WORKER_URL, { scope: '/proxy-webapp/' });\n    }\n    catch (error) {\n        workerRegistrationErrors.push(getErrorAdditionalData(error));\n        processServiceWorkerFailure();\n        return;\n    }\n    try {\n        await worker.update();\n        await navigator.serviceWorker.ready;\n    }\n    catch (error) {\n        workerRegistrationErrors.push(getErrorAdditionalData(error));\n        await worker?.unregister(); //       undefined,    Sentry - \n        processServiceWorkerFailure();\n        return;\n    }\n    navigator.serviceWorker.controller?.postMessage({ name: 'getStatus' });\n    navigator.serviceWorker.controller?.postMessage({ name: 'appConfig', data: appConfig });\n    /**\n     *       \n     *   -    \n     */\n    disconnect();\n    /**\n     * ServiceWorker   idle-  30 \n     *      -   \n     *     5 \n     */\n    const pingIntervalHandler = setInterval(() => {\n        if (!navigator.serviceWorker.controller) {\n            clearInterval(pingIntervalHandler);\n            return;\n        }\n        navigator.serviceWorker.controller.postMessage({ name: 'ping' });\n    }, SERVICE_WORKER_PING_TIMEOUT);\n};\nwindow.addEventListener('load', async () => {\n    //    ,   \n    //    ,      \n    if (!navigator.cookieEnabled) {\n        return;\n    }\n    try {\n        appConfig = await getAppConfig();\n    }\n    catch (error) {\n        /**\n         *     \n         *       \n         *\n         *    ,\n         *           \n         */\n        setTimeout(() => {\n            window.location.reload();\n        }, getReloadDelay());\n        return;\n    }\n    initSentry(appConfig);\n    if (isServiceWorkersAvailable()) {\n        registerWorker();\n    }\n    else {\n        //  -  \n        //     \n        initDirect();\n    }\n    setInterval(async () => {\n        try {\n            const newAppConfig = await getAppConfig(updateErrors === UPDATE_CONFIG_ERRORS_THRESHOLD);\n            if (newAppConfig.appVersion !== appConfig.appVersion) {\n                window.location.reload();\n            }\n            updateErrors = 0;\n        }\n        catch (error) {\n            updateErrors += 1;\n            if (updateErrors > UPDATE_CONFIG_ERRORS_THRESHOLD) {\n                Debug.log('out', 'Failed to update webapp config', getErrorAdditionalData(error));\n            }\n        }\n    }, PROXY_WEBAPP_CONFIG_UPDATE_INTERVAL);\n});\n"],"sourceRoot":""}