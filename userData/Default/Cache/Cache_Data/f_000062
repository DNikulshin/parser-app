 | string, SkillsCompetenciesGroup>>((groups, skill) => {\n        const appropriateCompetence = currentRoleCompetenceList.find(\n            (competence) =>\n                competence.skills.findIndex(\n                    (competenceSkill) => competenceSkill.toLowerCase() === skill.name.toLocaleLowerCase()\n                ) !== -1\n        );\n\n        if (appropriateCompetence) {\n            if (groups[appropriateCompetence.id]) {\n                groups[appropriateCompetence.id].skills.push(skill);\n            } else {\n                groups[appropriateCompetence.id] = {\n                    ...appropriateCompetence,\n                    skills: [skill],\n                };\n            }\n        } else if (groups.other) {\n            groups.other.skills.push(skill);\n        } else {\n            groups.other = {\n                id: 'other',\n                title: trls[TrlKeys.other],\n                skills: [skill],\n            };\n        }\n\n        return groups;\n    }, {});\n\n    return (\n        <div className=\"key-skills-row\" data-qa=\"tags-key-skills\">\n            {Object.values(competenciesGroups).map((competence, index) => {\n                if (competence.id === 'other') {\n                    return null;\n                }\n\n                return (\n                    <Fragment key={competence.id}>\n                        {index > 0 && <VSpacing base={4} />}\n                        <KeySkillsCompetenceItem\n                            competence={competence}\n                            removable={removable}\n                            changeable={changeable}\n                            disabledSkills={disabledSkills}\n                            isStrongTitles={isStrongTitles}\n                            onTagRemove={onTagRemove}\n                            onSkillCountIncrease={onSkillCountIncrease}\n                            onSkillCountDecrease={onSkillCountDecrease}\n                        />\n                  