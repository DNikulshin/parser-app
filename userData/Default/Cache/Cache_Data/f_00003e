Prop,\n                ...activatorProps,\n                onClick: onActivatorClick,\n                selectedOptions,\n                multiple,\n                searchValue,\n                setSearchValue,\n                visible,\n                disabled,\n                placeholder,\n            })}\n            <BottomSheet\n                allowScrollWhileFocused\n                keyboardOverlaysContent={false}\n                visible={visible}\n                onClose={() => {\n                    setVisible(false);\n                    setLastValues();\n                }}\n                height={searchable ? BottomSheetHeight.FullScreen : bottomSheetHeight}\n                header={\n                    <NavigationBar\n                        title={placeholder}\n                        right={\n                            showCloseButtonInNavigationBar\n                                ? [\n                                      {\n                                          key: 'close',\n                                          icon: CrossOutlinedSize24,\n                                          onClick: () => setVisible(false),\n                                      },\n                                  ]\n                                : undefined\n                        }\n                        options={searchable && <SearchInput clearable value={searchValue} onChange={setSearchValue} />}\n                    />\n                }\n                footer={\n                    multiple || showClearButton ? (\n                        <BottomSheetFooter>\n                            {clearButton}\n                            {applyButton}\n                        </BottomSheetFooter>\n                    ) : null\n                }\n            >\n                <div\n                    data-qa=\"magritte-select-option-list-on-xs\"\n                    className={`${styles.selectOptions} ${styles['select-options-xs']}`}\n                    re