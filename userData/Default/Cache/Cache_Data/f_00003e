*    */\n    children?: ButtonLabel;\n    /**    */\n    counter?: string;\n    /**    */\n    hideLabel?: boolean;\n    /**  */\n    icon?: ReactElement;\n    /**      */\n    stretched?: boolean;\n    /**   ,    size = Large  Medium */\n    subcaption?: ReactNode;\n}\n\ninterface ButtonElementProps {\n    /**       [ButtonMode](#button-mode) */\n    mode: ButtonMode;\n    /**       */\n    style?: ButtonStyle;\n    /**    */\n    disabled?: boolean;\n    /**   'a', 'button', 'span'   */\n    Element?: 'a' | 'button' | 'span';\n    /**    */\n    loading?: boolean;\n    /** onClick callback */\n    onClick?: (event: React.MouseEvent<HTMLElement, MouseEvent>) => void;\n    /**    DOM   */\n    ref?: React.Ref<HTMLElement>;\n    /**     . */\n    size?: ButtonSize;\n    /**     */\n    type?: ButtonType;\n    /** QA    */\n    'data-qa'?: string;\n}\n\nexport type DefaultButtonProps = Override<\n    VariableButtonProps,\n    {\n        ariaLabel?: never;\n        children: ButtonLabel;\n        hideLabel?: never;\n        icon?: never;\n        subcaption?: never;\n    }\n>;\n\nexport type SubcaptionButton = Override<\n    VariableButtonProps,\n    {\n        ariaLabel?: never;\n        children: string;\n        counter?: never;\n        hideLabel?: never;\n        icon?: never;\n        subcaption: ReactNode;\n    }\n>;\n\nexport type IconButton<IconType, HideLabel> = Override<\n    VariableButtonProps,\n    {\n        ariaLabel: string;\n        children?: ButtonLabel;\n        hideLabel: HideLabel;\n        icon: IconType;\n        stretched?: never;\n        subcaption?: never;\n    }\n>;\n\nexport type LabeledIconButton<IconType, HideLabel> = Override<\n    VariableButtonProps,\n    {\n        ariaLabel?: string;\n        children: ButtonLabel;\n        hideLabel?: HideLabel;\n        icon: IconType;\n        subcaption?: never;\n    }\n>;\n\n//    \nexport type AcceptableButtonPropsSets =\n    | SubcaptionButton\n    | IconButton<ReactElement, true>\n    | Lab