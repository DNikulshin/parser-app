     in={visible}\n                        onEntered={() => {\n                            setLockClickOutside(false);\n                        }}\n                        timeout={animationTimeout}\n                        classNames={animationClassNames}\n                        unmountOnExit\n                    >\n                        <div\n                            {...props}\n                            data-qa=\"drop-base\"\n                            tabIndex={autoFocusWhenOpened ? 0 : -1}\n                            className={styles.dropBase}\n                            ref={dropNodeRefMulti}\n                            style={{\n                                top: `${metrics.top}px`,\n                                left: `${metrics.left}px`,\n                            }}\n                            role={role}\n                        >\n                            {children(position, {\n                                metrics,\n                                dropScrollableContainerRef,\n                            })}\n                        </div>\n                    </CSSTransition>,\n                    host?.current ?? document.body\n                )}\n        </>\n    );\n};\n\nconst DropBase = forwardRef(DropBaseComponent) as (<\n    T extends\n        | DropBaseHorizontalPosition\n        | DropBaseVerticalPosition\n        | DropBaseHorizontalPositionWithoutCenter\n        | DropBaseVerticalPositionWithoutCenter\n>(\n    props: DropBaseProps<T> & { ref?: ForwardedRef<HTMLElement> }\n) => ReactElement) & { displayName: string };\n\nDropBase.displayName = 'DropBase';\n\nexport { DropBase };\n","import { type RefObject, useCallback, useEffect, useRef } from 'react';\n\nexport const useCloseByClickOutside = (\n    ref: RefObject<HTMLElement>,\n    callback: (event: MouseEvent) => void,\n    closeByClickOutsideEnabled: boolean\n): void => {\n    const refCallback = useRef(callback);\n\n    const handleClick = useCallback(\n        (e